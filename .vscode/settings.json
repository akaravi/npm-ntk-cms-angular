{
  "cSpell.words": [
    "Alireza",
    "filemanager",
    "hypershop",
    "Karavi",
    "Typscript"
  ],
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit",
    "source.fixAll": "explicit",
    "source.removeUnusedImports": "explicit"
  },
  "typescript.preferences.organizeImports": "on",
  "typescript.suggest.autoImports": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#55e300",
    "activityBar.background": "#55e300",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#4087ff",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#55e300",
    "statusBar.background": "#42b000",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#55e300",
    "statusBarItem.remoteBackground": "#42b000",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#42b000",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#42b00099",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#42b000"
}
